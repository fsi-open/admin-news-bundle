{
    "name": "fsi/admin-news-bundle",
    "type": "symfony-bundle",
    "description": "FSiAdminBundle configuration for fsi/news-bundle",
    "keywords": ["news", "symfony2", "bundle", "fsi", "admin"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@fsi.pl"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "doctrine/orm": "~2.3",
        "doctrine/doctrine-bundle": "dev-master",
        "symfony/framework-bundle": "~2.3",
        "symfony/twig-bundle": "~2.3",
        "egeloen/ckeditor-bundle": "~2.0",
        "fsi/admin-bundle" : "1.1.x-dev",
        "fsi/news-bundle": "dev-master"
    },
    "require-dev": {
        "symfony/validator": "~2.3",
        "fzaninotto/faker": "v1.3.0",
        "phpspec/phpspec": "~2.0",
        "behat/behat": "~2.5.0",
        "behat/mink": "1.5.*",
        "behat/mink-extension": "*",
        "behat/mink-browserkit-driver": "1.1.*",
        "behat/mink-selenium2-driver": "1.1.*",
        "behat/symfony2-extension": "*",
        "sensiolabs/behat-page-object-extension": "1.0.4",
        "bossa/phpspec2-expect": "*@dev"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "FSi\\Bundle\\AdminNewsBundle\\": ""
        }
    },
    "target-dir" : "FSi/Bundle/AdminNewsBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
